{
  "name": "squalid-star",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "dev:chatkit": "node server/chatkit-dev.mjs",
    "dev:local": "concurrently -k -n api,web -c yellow,cyan \"npm:dev:chatkit\" \"npm:dev\"",
    "verify:backend-types": "npx tsc --pretty false --noEmit -p amplify/tsconfig.json",
    "prebuild": "node scripts/validate-i18n.mjs",
    "predeploy": "npm run verify:backend-types",
    "build": "astro build",
    "indexnow:submit": "node scripts/indexnow-submit.mjs",
    "preview": "astro preview",
    "astro": "astro"
  },
  "dependencies": {
