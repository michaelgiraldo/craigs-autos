{
  "name": "squalid-star",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "dev:chatkit": "node --env-file-if-exists=.env.local server/chatkit-dev.mjs",
    "dev:local": "concurrently -k -n api,web -c yellow,cyan \"npm:dev:chatkit\" \"npm:dev\"",
    "verify:backend-types": "npx tsc --pretty false --noEmit -p amplify/tsconfig.json",
    "test:backend": "node --experimental-strip-types --test --test-concurrency=1 \"amplify/functions/**/*.test.ts\"",
    "verify:backend": "npm run verify:backend-types && npm run test:backend",
    "lint": "biome lint amplify/functions server scripts shared src/scripts",
    "format:check": "biome check --formatter-enabled=true --linter-enabled=false --assist-enabled=false amplify/functions server scripts shared src/scripts",
    "generate:og": "node scripts/generate-og-images.mjs",
    "prebuild": "node scripts/validate-i18n.mjs && npm run generate:og",
    "predeploy": "npm run verify:backend && npm run lint && npm run format:check",
    "build": "rm -rf dist && astro build",
    "indexnow:submit": "node scripts/indexnow-submit.mjs",
    "preview": "astro preview",
    "astro": "astro"
  },
  "dependencies": {
    "@astrojs/mdx": "^4.3.13",
    "@astrojs/react": "^4.4.2",
    "@astrojs/sitemap": "^3.7.0",
    "@aws-amplify/plugin-types": "^1.11.2",
    "@aws-sdk/client-dynamodb": "^3.995.0",
    "@aws-sdk/client-scheduler": "^3.995.0",
    "@aws-sdk/client-sesv2": "^3.995.0",
    "@aws-sdk/lib-dynamodb": "^3.995.0",
    "@aws-sdk/types": "^3.973.1",
    "@openai/chatkit": "^1.6.0",
    "@openai/chatkit-react": "^1.5.0",
    "@resvg/resvg-js": "^2.6.2",
    "astro": "^5.17.3",
    "concurrently": "^9.2.1",
    "openai": "^6.22.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "sharp": "^0.34.5",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@aws-amplify/backend": "^1.21.0",
    "@aws-amplify/backend-cli": "^1.8.2",
    "@biomejs/biome": "^2.2.6",
    "aws-cdk-lib": "^2.239.0",
    "constructs": "^10.5.1",
    "esbuild": "^0.27.3",
    "glob": "^13.0.6",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
