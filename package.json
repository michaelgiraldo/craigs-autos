    "@aws-sdk/client-dynamodb": "^3.990.0",
    "@aws-sdk/client-sesv2": "^3.990.0",
    "@aws-sdk/lib-dynamodb": "^3.990.0",
    "@aws-sdk/types": "^3.973.1",
    "@openai/chatkit": "^1.6.0",
    "@openai/chatkit-react": "^1.5.0",
    "astro": "^5.17.2",
    "concurrently": "^9.2.1",
    "dotenv": "^17.3.1",
    "openai": "^6.22.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  },
  "devDependencies": {
    "@aws-amplify/backend": "^1.21.0",
